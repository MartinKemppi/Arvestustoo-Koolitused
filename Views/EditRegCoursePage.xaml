<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Koolitused.Views.EditRegCoursePage">
    <VerticalStackLayout Padding="30,0" Spacing="20">
        <Entry Placeholder="Eesnimi" x:Name="FirstNameEntry" />
        <Entry Placeholder="Perekonnanimi" x:Name="LastNameEntry" />
        <Entry Placeholder="Telefon" x:Name="PhoneEntry" />
        <Entry Placeholder="Email" x:Name="EmailEntry" />
        <Entry Placeholder="Lisa info" x:Name="AdditionalInfoEntry" />
        <Button Text="Registreeri" Clicked="OnRegisterClicked" />
    </VerticalStackLayout>
</ContentPage>
